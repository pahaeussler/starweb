<h1>Planets</h1>
<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <%# <th colspan="3"></th> %>
    </tr>
  </thead>

  <tbody>
    <% @planets.each do |edge| %>
      <% planet= edge['node'] %>
      <tr>
        <td><%= link_to planet['name'], planet_path(planet['id']) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
