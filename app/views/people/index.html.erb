<p id="notice"><%= notice %></p>

<h1>People</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <%# <th colspan="3"></th> %>
    </tr>
  </thead>

  <tbody>
    <% @people.each do |edge| %>
      <% person= edge['node'] %>
      <tr>
        <td><%= link_to person['name'], person_path(person['id']) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>